<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="false" >
        <shape >
            <corners android:radius="2dp"></corners>
            <solid android:color="@color/btn_unpress_gray"></solid>
            <stroke android:color="@color/btn_border_gray" android:width="1dp"></stroke>
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape >
            <corners android:radius="2dp"></corners>
            <solid android:color="@color/btn_press_gray"></solid>
            <stroke android:color="@color/btn_border_gray" android:width="1dp"></stroke>
        </shape>
    </item>
</selector>
